{
	"version": 2,
	"public": true,
	"alias": "minimal-css-service",
	"builds": [
		{ "src": "public/*", "use": "@now/static" },
		{
			"src": "index.js",
			"use": "@now/node",
			"config": { "maxLambdaSize": "40mb" }
		}
	],
	"routes": [
		{ "src": "/", "dest": "/public/index.html" },
		{ "src": "/(.+)", "dest": "/index.js" }
	]
}
